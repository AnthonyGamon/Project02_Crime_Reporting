<div class="main" data-id:"{{user.id}}">
  <div class="container-of-form">
  <form class="report-form" >
    <div class="formcontainer">
      <h1>Report a crime form</h1>
    <div class="container">
      <!--EMAIL-->
      <label for="email"><h2>What is your email?</h2></label>
      <input id="email" type="text" placeholder="Enter email" name="email" required>
      <br>
      <!--TITLE-->
      <label for="title"><h2>Title of the crime</h2></label>
      <input id="title" type="text" placeholder="Title of crime" name="email" required>
      <br>
      <!--BODY-->
      <label for="body"><h2>What happened?</h2></label>
      <input id="body" type="text" placeholder="Let us know what happened?" name="body" required>
      <br>
      <!--LATITUDE AND LONGITUDE-->
      <label for="latAndLon"><h2>Where did the crime happen?</h2></label>
      <input id="latAndLon" type="text" placeholder="Where did the crime happen?" name="latAndLon" required>
    </div>
    <button type="submit" class="submitBtn"><strong>Submit</strong></button>
    </form>
  </div>
  <br>
  <br>

    <ul class="cards">
      {{#each crime}}
        {{> crimes/crime-card }}
      {{/each}}
    </ul>
</div>
